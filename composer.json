{
  "name": "prometee/sylius-payum-monetico-plugin",
  "description": "Payum Monetico Bridge for Sylius",
  "type": "sylius-plugin",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Prometee/PayumMonetico"
    }
  ],
  "require": {
    "sylius/sylius": "^1.2",
    "payum/payum": "^1.5",
    "ekyna/payum-monetico": "dev-master"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Francis Hilaire",
      "email": "Prometee@users.noreply.github.com"
    }
  ],
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Prometee\\SyliusPayumMoneticoPlugin\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
