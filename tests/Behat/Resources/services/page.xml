<?xml version="1.0" encoding="UTF-8" ?>

<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">
    <services>

        <service id="tests.prometee.sylius_payum_monetico_plugin.behat.page.admin.payment_method.create"
                 parent="sylius.behat.page.admin.payment_method.create"
                 class="Tests\Prometee\SyliusPayumMoneticoPlugin\Behat\Page\Admin\PaymentMethod\CreatePage">
        </service>

        <service id="tests.prometee.sylius_payum_monetico_plugin.behat.page_checkout.external"
                 parent="sylius.behat.page"
                 class="Tests\Prometee\SyliusPayumMoneticoPlugin\Behat\Page\External\MoneticoCheckoutPage">
            <argument type="service" id="sylius.repository.payment_security_token" />
            <argument type="service" id="test.client" />
            <argument type="service" id="tests.prometee.sylius_payum_monetico_plugin.behat.page.monetico.notify_page" />
            <argument type="service" id="sylius.repository.payment" />
        </service>

        <service id="tests.prometee.sylius_payum_monetico_plugin.behat.page.monetico.notify_page"
                 parent="sylius.behat.symfony_page"
                 class="Tests\Prometee\SyliusPayumMoneticoPlugin\Behat\Page\Monetico\MoneticoNotifyPage">
        </service>
    </services>
</container>
